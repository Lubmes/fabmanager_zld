<div class="w3-row">
  <h1 class="w3-round-xlarge w3-lime w3-padding" style="font-weight: bold">Agenda</h1>
<div class="w3-padding w3-col m6 m3">
    <div class="w3-rightbar w3-border-blue-gray">
      <div class="w3-left w3-row-padding w3-padding-left w3-card w3-margin-bottom w3-round-xlarge">
          <div class="w3-topbar w3-border-blue-gray w3-margin-top w3-large">
            <%= render "events/eventlist", approved: true %>
          </div>
        <div>

        </div>
        <div class="w3-topbar w3-border-blue-gray">
          <td>
            <%= link_to new_event_path, class:"btn w3-lime w3-round-xlarge w3-margin-bottom w3-padding w3-margin-top w3-topbar w3-border-blue-gray w3-hover-blue"  do %>Nieuwe Reservering
              <i class="fa fa-plus" aria-hidden="true"></i>
            <% end %>
          </td>
        </div>
      </div>
    </div>
</div>
  <div class="w3-row-padding w3-left w3-col m6 w3-padding-left w3-card w3-round-xlarge" style="height: 100%;">
    <div id="calendar" class="w3-text-blue-gray w3-margin-bottom" style="width: 70%;">
      <%= render "events/calendar", events: @events %>
    </div>
  </div>
</div>