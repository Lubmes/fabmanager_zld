<%= simple_form_for @fabmoment do |f| %>
	
	<!-- Input -->
  <%= f.input_field :title %>
  <%= f.label :title %>
  <%= f.error :title %>
	<%= f.input_field :description %>
  <%= f.label :description %>
	<%= f.error :description %>
  <br><br>
  <!-- Zenden -->
  <%= f.button :button, class: "w3-btn w3-blue w3-center" %>
  
<% end %>