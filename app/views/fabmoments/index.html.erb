<div class="w3-container w3-center w3-padding-xlarge">
	<header class="w3-container w3-margin-bottom w3-margin-top">
    <!--<img src="https://www.fablabs.io/media/W1siZiIsIjIwMTcvMDIvMjYvMTQvMDcvMTkvMGY4ZTViNmMtNDNiYi00MDljLTg2Y2UtN2Q0YmVkYjdhZTFiL0lNR183NTg1LkpQRyJdLFsicCIsInRodW1iIiwiMzAweDMwMCJdXQ/IMG_7585.JPG?sha=513c5a1bbc8e58bf" class="w3-center">-->
    <div class="w3-row-padding">
      Dit is de website van FabLab Zeeland
      <div class="w3-card shader w3-padding-top w3-padding-bottom w3-margin-bottom w3-margin-top w3-margin-left w3-margin-right">
          <img src="/assets/FablabHeader.jpg" alt="Fablab Header">
      </div>
      <div style='width: 800px;'>
        <div id="one_marker" style='width: 800px; height: 400px;'></div>
      </div>


      <div class="openingtimes-ajax w3-left w3-row-padding w3-card w3-margin-bottom">
        <%= render "admin/openingtimes/openingtime" %>
      </div>
      <div class="w3-right w3-row-padding w3-card w3-margin-bottom">
        <%= render "fabmoments/countdown_timer", fabmoments: @fabmoments %>
      </div>


    </div>
    <%= form_tag search_fabmoments_path,
						method: :get,
		        class: "w3-right-align" do %>
			        <%= text_field_tag "search", params[:search], class: "w3-form" %>
			        <%= submit_tag "Zoek", class: "w3-btn w3-indigo" %>
	      <% end %>
    </header>
    <footer class="w3-center w3-topbar w3-border-blue-grey w3-padding-xlarge">
  <%= render "fabmoments/fabmoments_gallery", fabmoments: @fabmoments %>
    </footer>
</div>