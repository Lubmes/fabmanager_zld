      <!--</div>-->

      <div class="w3-left jumbotron-icon">
        <!--<img src="/placeholder.png" alt="MISSING PNG">-->
        <!--<img id="ImgFab" src="/images/placeholder.png" alt="MISSING PNG" />-->
        <i class="fa fa-6 fa-caret-left w3-text-lime" aria-hidden="true"></i>
        <img src="http://www.jennybeaumont.com/wp-content/uploads/2015/03/placeholder.gif"/>
        <i class="fa fa-6 fa-caret-right w3-text-lime" aria-hidden="true"></i>
    </div>
      <div id="calendar" class="w3-right w3-row-padding w3-padding-left w3-card w3-margin-bottom">
      <%= render "events/calendar" %>
      </div>
      <div class="w3-row-padding w3-padding-left w3-margin-bottom">
        <td><%= link_to new_event_path, class:"w3-button w3-circle w3-green"  do %>
              <i class="fa fa-plus" aria-hidden="true"></i>
          <% end %></td></div>

      <!--<div style='width: 800px;'>-->
