<%= simple_form_for(@meeting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :rails %>
    <%= f.input :g %>
    <%= f.input :scaffold %>
    <%= f.input :Meeting %>
    <%= f.input :name %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :calendar %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
